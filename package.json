    "jshint": "^2.9.5",
    "saucelabs": "^1.4.0",
    "steal": "^1.0.5",
    "steal-qunit": "^1.0.0",
    "steal-tools": "^1.0.1",
    "test-saucelabs": "0.0.1",
    "testee": "^0.3.2"
